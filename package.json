  "scripts": {
    "build": "nest build && npm run copy && npm run copy-dist",
    "copy": "copy \"package.json\" \"dist/\" && copy \"web.config\" \"dist/\" && copy \".env.production\" \"dist/\"",
    "copy-dist": "copyfiles dist/**/* D:/workspace/Be-Lib_Build/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": " npm run format && cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
